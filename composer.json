{
    "name": "php-saas/cli",
    "description": "The command line interface for the PHP-SaaS project",
    "keywords": ["php", "laravel", "php-saas", "cli", "saas", "starter-kit"],
    "homepage": "https://php-saas.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/php-saas/cli/issues",
        "source": "https://github.com/php-saas/cli"
    },
    "authors": [
        {
            "name": "Saeed Vaziry",
            "email": "mr.saeedvaziry@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel-zero/framework": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["php-saas"]
}
