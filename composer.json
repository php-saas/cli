{
    "name": "php-saas/cli",
    "description": "The command line interface for the PHP-SaaS project",
    "keywords": ["php", "laravel", "php-saas", "cli", "saas", "starter-kit"],
    "homepage": "https://php-saas.com",
    "support": {
        "issues": "https://github.com/php-saas/cli/issues",
        "source": "https://github.com/php-saas/cli"
    },
    "authors": [
        {
            "name": "Saeed Vaziry",
            "email": "mr.saeedvaziry@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/filesystem": "^10.20|^11.0|^12.0",
        "illuminate/support": "^10.20|^11.0|^12.0",
        "laravel/prompts": "^0.1.18|^0.2.0|^0.3.0",
        "symfony/console": "^6.2|^7.0",
        "symfony/process": "^6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "PHPSaaS\\Cli\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["php-saas"],
    "require-dev": {
        "laravel/pint": "^1.24"
    }
}
